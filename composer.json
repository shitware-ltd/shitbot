    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/support": "^10.0",
        "react/async": "^4.0",
        "react/event-loop": "^1.3",
        "react/http": "^1.7",
        "team-reflex/discord-php": "^7.1",
        "vlucas/phpdotenv": "^5.5"
    }
}
