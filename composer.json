{
    "name": "shitware-ltd/shitbot",
    "description": "A discord bot written in php. Not shit.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ShitwareLtd\\Shitbot\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Richard Tippin",
            "email": "tippindev@gmail.com"
        }
